<!-- 이중 반복문  -->
<!-- 세로 가로 구분 -->
<!-- 문자열 바꿔주기 -->
<body>
    <script>
        function solution(n, m) {
            let result = "";
            for (let i = 0; i < m; i++) {
                for (let j = 0; j < n; j++) {
                    result += "*";
                   
                }
                result += "\n";
                
            }
            return result;
        }
        console.log(solution(5, 3));
    </script>
</body>
</html>
